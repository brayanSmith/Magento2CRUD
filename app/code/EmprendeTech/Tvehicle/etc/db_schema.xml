<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="emprendetech_tvehicle_type" resource="default" engine="innodb" comment="emprendetech_tvehicle_type Table">
        <column xsi:type="int" name="type_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="type_id"/>
        </constraint>
        <column name="description" nullable="false" xsi:type="varchar" comment="Description Type" length="255"/>
        <column name="is_active" nullable="false" xsi:type="smallint" comment="Is Active Blog"/>
        <column name="created_at" nullable="false" xsi:type="timestamp" comment="Created At" default="CURRENT_TIMESTAMP"/>
    </table>
</schema>
